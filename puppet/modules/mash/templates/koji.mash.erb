# mash config file

<% @tags.each_pair do |tag,properties| %>
[<%= tag %>]
rpm_path = %(arch)s/
repodata_path = %(arch)s/
source_path = source/SRPMS
debuginfo = True
multilib = True
multilib_method = devel
tag = <%= tag %>
inherit = True
strict_keys = False
keys = <%= properties['gpgkeys'] %>

repoviewurl = http://<%= hub %>/mash/
repoviewtitle = "<%= tag %> - %(arch)s"
arches = <%= arch %>
delta = True
# Change distro_tags as fedora-release version gets bumped
distro_tags = cpe:/o:<%= operatingsystem %>:<%= operatingsystem %>:<%= operatingsystemrelease %> <%= tag %>
hash_packages = False

<% end %>


